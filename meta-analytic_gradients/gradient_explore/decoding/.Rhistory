geom_boxplot(aes(x = -.2),
outlier.alpha = 0, alpha = .7, width = .1, colour = "black", fill = "#2AA27A") +
geom_point(aes(y = 0.7149375090758509632), size = 10, shape = 8, colour = "#2AA27A") +
geom_point(aes(y = 0.7681538741375886215), size = 10, shape = 8, colour = "darkorange2") +
scale_y_continuous(limits = c(0.5, 1)) +
